<script type="text/javascript">
    $(document).ready(function () {
        $('.btn-back').click(function(evt){
            evt.preventDefault();
             window.history.back();
        });
        $('#id_birth_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_death_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_start_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_end_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_init_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_apply_date').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#id_date_scheduled').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('.datepicker').datepicker({
            dateFormat: "yy-mm-dd"
        });
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
        });

    });

    function formToDict(elem) {
        var formArray = $(elem).serializeArray();
        var data = {};

        for (var i = 0; i < formArray.length; i++) {
            var row = formArray[i];
            data[row.name] = row.value;
        }

        return data;
    }
</script>




